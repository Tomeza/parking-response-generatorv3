    "w":  { "車高":3,"車幅":3,"外車":3,"制限":2,"軽自動車":2,"大型":2 }
  },
  "facility": {
    "pos": ["設備","施設","精算機","ゲート","照明","防犯","カメラ","屋根","充電","バリアフリー","利用時間","営業時間","開館","閉館"],
