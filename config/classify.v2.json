{
  "_global": {
    "stop": ["利用","利用方法","利用時間","確認","教えて","情報","詳細"]
  },
  "_overrides": [
    { "if": "(料金|支払|精算|価格|費用)", "route": "payment" },
    { "if": "(受け入れ|不可|制限|上限|規格|寸法)", "and": "(車両|車種|外車|軽自動車|車高|車幅|重量)", "route": "vehicle" },
    { "if": "(設備|施設|バリアフリー)", "route": "facility" },
    { "if": "(車両|車種|外車|軽自動車).*(料金|価格|費用)", "route": "vehicle" },
    { "if": "(精算機|ゲート)", "route": "facility" },
    { "if": "(駐車券|チケット).*(紛失|無くした|落とした)", "route": "trouble" }
  ],
  "_intent": [
    { "if": "(変更手続|変更方法)", "route": "reservation/modify" },
    { "if": "(設定方法|手順|やり方)", "setIntent": "check" }
  ],
  "access": {
    "pos": ["アクセス","行き方","住所","地図","Google","Map","ナビ","ルート","場所","所在地","最寄","道順","左折","右折","信号","交差点","入口","出口"],
    "neg": ["送迎","迎え","ピックアップ","無料送迎","料金","支払い"],
    "phr": ["Google Map","Googleマップ","どう行けば","行き方 教えて","ナビ 設定"],
    "w":  { "Google":3, "Map":3, "ナビ":2, "住所":2, "行き方":2 }
  },
  "shuttle": {
    "pos": ["送迎","迎え","ピックアップ","無料送迎","空港まで","定員","ひとり送迎","待ち時間","到着後","荷物受取後","国内線","出発便","到着便"],
    "neg": ["予約"],
    "phr": ["荷物 受取 後 連絡","空港 到着 後 電話","ひとり送迎"],
    "w":  { "送迎":3, "無料送迎":3, "空港まで":2, "定員":2, "到着後":2 }
  },
  "payment": {
    "pos": ["料金","支払い","現金","領収書","請求","精算","価格","費用","決済"],
