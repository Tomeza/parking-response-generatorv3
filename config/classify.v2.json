  "reservation": {
    "pos": ["予約","締切","ネット予約","キャンセル","変更","確認","予約状況","空き","満車","満室"],
    "neg": ["現金","領収書","送迎","定員"],
    "phr": ["前日 22:00 まで","ご予約 ページ","予約 完了 画面"],
    "w":  { "予約":3, "ネット予約":3, "締切":2, "キャンセル":2 }
  },
  "vehicle": {
    "pos": ["車両","車種","外車","大型","軽自動車","車高","車幅","重量","受け入れ","不可","規格","サイズ","鍵","預かり","付属品","汚れ","管理","駐車券","紛失","制限","規制","上限","寸法"],
    "neg": ["送迎","予約","精算"],
