  "trouble": {
    "pos": ["トラブル","クレーム","返金","故障","遅延","事故","問題","紛失","破損","迷惑"],
    "neg": [],
    "phr": ["返金 対応","対応 お願い","間違い 送迎"],
    "w":  { "クレーム":3, "返金":3, "故障":2, "事故":2 }
  },
  "information": {
    "pos": ["記入","入力","フォーム","フライト","便名","到着","出発","情報","詳細"],
    "neg": [],
    "phr": ["便 未定","フライト 情報"],
    "w":  { "フライト":2, "便名":2 }
  },
  "_tagBoost": {
    "送迎関連": 2,
    "料金関連": 2
  }
